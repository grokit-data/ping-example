{
  "name": "ts-paranet-agent",
  "version": "1.1.7",
  "description": "",
  "scripts": {
    "awt": "ts-node src/awt-sim.ts",
    "truck": "ts-node src/examples/truck.ts",
    "arrays": "ts-node src/examples/arrays.ts",
    "chat": "ts-node src/examples/chat.ts",
    "hello": "ts-node --esm src/examples/hello.ts",
    "simtest": "ts-node --esm src/examples/simtest.ts",
    "requester": "ts-node --esm src/examples/requester.ts",
    "build": "tsc -p . --outDir build",
    "export": "npm run build && node -e \"require('./build/compilemode.js');require('./build/chat.js');\"",
    "rollup": "rollup -c --bundleConfigAsCjs"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "graphql": "^15.8.0",
    "graphql-http": "^1.22.0",
    "reflect-metadata": "^0.1.13"
  },
  "author": "Scott Thibault",
  "license": "UNLICENSED",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.6",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}
